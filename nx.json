{
  "npmScope": "bg-hoard",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".github/workflows/ci.yml": "*",
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ],
        "accessToken": "MDYzZmU2MzAtODg1Ni00NDgxLWI5NTgtZGMwOWRhYWU2ZGZlfHJlYWQtd3JpdGU=",
        "canTrackAnalytics": false,
        "showUsageWarnings": true
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "api": {
      "tags": [
        "scope:api",
        "type:app",
        "scope:new"
      ]
    },
    "api-util-notifications": {
      "tags": [
        "type:util",
        "scope:api"
      ]
    },
    "store": {
      "tags": [
        "scope:store",
        "type:app"
      ]
    },
    "store-e2e": {
      "tags": [
        "scope:store",
        "type:e2e"
      ],
      "implicitDependencies": [
        "store"
      ]
    },
    "store-feature-game-detail": {
      "tags": [
        "scope:store",
        "type:feature"
      ]
    },
    "store-ui-shared": {
      "tags": [
        "scope:store",
        "type:ui"
      ]
    },
    "store-ui-shared-e2e": {
      "tags": [
        "scope:store",
        "type:e2e"
      ],
      "implicitDependencies": [
        "store-ui-shared"
      ]
    },
    "store-util-formatters": {
      "tags": [
        "scope:store",
        "type:util"
      ]
    },
    "util-interface": {
      "tags": [
        "scope:shared",
        "type:util"
      ]
    }
  }
}
